{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", 
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [true, 2],
        "quotemark": [false],
        "ordered-imports":[false],
        "forin": false,
        "object-literal-sort-keys": false,
        "prefer-const": false,
        "no-console": [
            true,
            "log"
        ],
        "arrow-parens": false
    },
    "rulesDirectory": []
}
